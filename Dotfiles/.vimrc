set number
set numberwidth=6
set showmatch
set smartcase
set list
"set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:%
set listchars=tab:>-,trail:-,eol:$
set tabstop=4
set shiftwidth=4
if has("syntax")
	syntax on
endif


set encoding=utf-8
set termencoding=utf-8
set fileencodings=utf-8,iso-2022-jp,cp932,sjis,euc-jp
set fileformats=unix,dos,mac

set ambiwidth=double


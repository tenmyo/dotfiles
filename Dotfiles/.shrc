# ~/.shrc: exec on interactive(generic)
# shell options, aliases, functions, completions

# If not running interactivery, don't do anything
[ "$-" != "*i*" ] && return

echo .shrc

# ===============================$
# Shell Options$
# ===============================$
# Can't overwrite by redirect$
# If you want to force overwrite, use >| instead >$
set -o noclobber$


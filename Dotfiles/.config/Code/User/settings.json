{
    "editor.minimap.renderCharacters": false,
    "editor.mouseWheelZoom": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.renderIndentGuides": true,
    "editor.rulers": [80],
    "editor.scrollBeyondLastLine": false,
    "files.insertFinalNewline": true,
    "git.autofetch": true,
    "jupyter.appendResults": false,
    "python.autoComplete.addBrackets": true,
    "python.autoComplete.preloadModules": [
        "pandas",
        "numpy",
        "matplotlib"
    ],
    "python.formatting.provider": "yapf",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.mypyArgs": [
        "--ignore-missing-imports",
        "--follow-imports=silent",
        "--strict",
        "--strict-optional"
    ],
    "[cmake]": {
        "editor.tabSize": 2
    },
    "cmake-tools-helper.cmake_download_path": "/home/tenmyo/.vscode/extensions/maddouri.cmake-tools-helper-0.1.1/cmake_download",
    "cmake.cmakePath": "/home/tenmyo/.vscode/extensions/maddouri.cmake-tools-helper-0.1.1/cmake_download/cmake-3.8.2-Linux-x86_64/bin/cmake",
    "cmake.useCMakeServer": true,
    "workbench.iconTheme": "vscode-icons",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "TodoParser": {
        //"exclude": ["cpp", "c"],
        //"include": ["h","c","cpp","txt"],
        "folderExclude": ["node_modules", ".vscode"],
        //"only": ["sub-folder/sub-sub-folder"],
        "showInProblems": true,
        //"markers": ["TODO:","NOTE:", "REMINDER:", ["FIXME", "Warning"]],
    },
}
